<script>

    var aid = '5024';

    function pageJS() {
        var w = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth;
        var bg = $('.indienova-title-bg');
        if (w <= 991) {
            bg.attr('data-stellar-vertical-offset', '226');
        }

        var h4s = $('.indienova-single-post h4');
        if (h4s.length > 0) {
            $('#side-title-image').after('<div class="scrollspy"><ul id="indienova-header-list" class="nav hidden-xs hidden-sm hidden-ms" data-spy="affix"></ul></div>');
            if ($('.indienova-single-post').html().trim().substr(1, 2) != 'h4') {
                $('.indienova-single-post').prepend('<h4>引言</h4>');
            }

            h4s = $('.indienova-single-post h4');
            for(var i=0; i<h4s.length; i++) {
                $(h4s[i]).attr('id', 'iah-' + i);
                var title = ($(h4s[i]).html()).replace(/\'/g, "").replace(/\"/g, "");
                title = title.replace(/<br>/g, " - ");
                title = title.replace(/<small>/g, "");
                title = title.replace(/<\/small>/g, "");
                title = title.replace(/<strong>/g, "");
                title = title.replace(/<\/strong>/g, "");
                $('#indienova-header-list').append('<li><a href="#iah-' + i + '" title="' + title + '">' + $(h4s[i]).html() + '</a></li>');
            }
            $('#indienova-header-list').append('<li><a href="#comments" title="直接查看文章评论"><i class="icon-chat-2"></i> 文章评论</a><li>');

            $('#indienova-header-list').affix({
                offset: {
                    top: $('#indienova-header-list').offset().top - 140,
                    bottom: ($('footer').outerHeight(true) + $('#indienova-misc').outerHeight(true)) + 180
                }
            });
        }

        var ocPortfolio = $("#oc-portfolio");

        ocPortfolio.owlCarousel({
            margin: 20,
            nav: true,
            stagePadding: 50,
            loop: 'true',
            navText: ['<i class="icon-angle-left"></i>','<i class="icon-angle-right"></i>'],
            autoplay: false,
            autoplayHoverPause: true,
            dots: false,
            responsive:{
            0:{ items:1 },
            600:{ items:2 },
            1000:{ items:3 }
        }
    });

        /*
                  */

        /* spoiler */

        $('.spoiler-div').on('click', function() {
            $(this).find('.spoiler-content').removeClass('spoiler-content');
            $(this).find('.spoiler-info').remove();
            $(this).removeClass('spoiler-div');
            $(this).unbind('click');
        });

        /* more users */
        $('#moreLikedUsers').on('click', function() {
            $(this).remove();
            $('.hidden-user').removeClass('hidden-user');
        });

        /* more games */
        $('#moreRelatedGames').on('click', function() {
            $('.related-games-holder').find('div').removeClass('initHidden');
            $(this).parent().parent().remove();
        });
    }
